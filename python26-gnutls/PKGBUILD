# Maintainer: Daniele Cocca <jmc@chakra-project.org>

_pkgname=python-gnutls
pkgname=python26-gnutls
pkgver=1.2.0
pkgrel=1
pkgdesc="Python wrapper for the GNU TLS library."
arch=('any')
license=('LGPL')
depends=('python26')
url="http://ag-projects.com/"
source=(http://pypi.python.org/packages/source/p/${_pkgname}/${_pkgname}-${pkgver}.tar.gz)
md5sums=('18a7b92abdc1598b916d7ff5019b72bf')

package() {
  cd "${srcdir}/${_pkgname}-${pkgver}"
  python2.6 setup.py install --prefix=/usr --root="${pkgdir}" || return 1
}
