#
# build function
#
build()
{
	cd $startdir/src/

packs=('ar'
       'bg'
       'ca'
       'ca@valencia'
       'cs'
       'csb'
       'da'
       'de'
       'el'
       'en_GB'
       'eo'
       'es'
       'et'
       'eu'
       'fi'
       'fr'
       'fy'
       'ga'
       'gl'
       'gu'
       'he'
       'hi'
       'hr'
       'hu'
       'ia'
       'id'
       'is'
       'it'
       'ja'
       'kk'
       'km'
       'kn'
       'ko'
       'lt'
       'lv'
       'mai'
       'mk'
       'ml'
       'nb'
       'nds'
       'nl'
       'nn'
       'pa'
       'pl'
       'pt'
       'pt_BR'
       'ro'
       'ru'
       'si'
       'sk'
       'sl'
       'sr'
       'sv'
       'tg'
       'tr'
       'uk'
       'wa'
       'zh_CN'
       'zh_TW')

# 	for it in `find . -maxdepth 1 -type d|sed -e '/^\.$/d'` ; do
# 		pushd "$it"
# 		cmake -DCMAKE_INSTALL_PREFIX=${_installprefix} -DCMAKE_BUILD_TYPE=Release
# 		make || return 1
# 		make DESTDIR=${pkgdir} install
# 		popd
# 	done

 	for it in ${packs[@]} ; do
 		pushd "kde-l10n-$it-$pkgver"
 		cmake -DCMAKE_INSTALL_PREFIX=${_installprefix} -DCMAKE_BUILD_TYPE=Release
 		make || return 1
 		#make DESTDIR=${pkgdir} install
 		popd
 	done
}



#
# split-install functions
#
package_kde-l10n-ar()
{
	pkgdesc="KDE language support: Arabic"
	groups=("kde-uninstall" "kde-l10n")

	pushd ${srcdir}/kde-l10n-ar-${pkgver}
		make DESTDIR=${pkgdir} install || return 1
	popd
}

