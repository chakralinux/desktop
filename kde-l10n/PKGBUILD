    pkgdesc="KDE language support: Xhosa"
    groups=("kde-uninstall" "kde-l10n")
    categories=('localization')

    pushd ${srcdir}/kde-l10n-xh-${pkgver}
        make DESTDIR=${pkgdir} install
    popd
}

