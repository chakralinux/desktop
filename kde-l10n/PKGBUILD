#
# KDE SC Packages for Chakra, part of chakra-project.org
#
# maintainer (i686): Phil Miller <philm[at]chakra-project[dog]org>
# maintainer (x86_64): Manuel Tortosa <manutortosa[at]chakra-project[dot]org>

# include global config
source ../_buildscripts/${current_repo}-${_arch}-cfg.conf



# original name (used for the source and pkgnames)
_origname=kde-l10n



#
# package info
#
pkgbase=('kde-l10n')
pkgname=('kde-l10n-nl')


arch=('any')
pkgver=${_kdever}
pkgrel=1
         
pkgdesc="split package"
url="http://www.kde.org"
license=('GPL' 'FDL')
depends=("kdelibs>=${_kdever}")
         
options=('docs' 'log')
         
         
source=(${_mirror}/kde-l10n/kde-l10n-nl-${pkgver}.tar.bz2)

md5sums=(
	`grep kde-l10n-nl-${pkgver}.tar.bz2 ../kde-sc.md5 | cut -d" " -f1`)

#
# build function
#
build()
{
	cd $startdir/src/

packs=('nl')

# 	for it in `find . -maxdepth 1 -type d|sed -e '/^\.$/d'` ; do
# 		pushd "$it"
# 		cmake -DCMAKE_INSTALL_PREFIX=${_installprefix} -DCMAKE_BUILD_TYPE=Release
# 		make || return 1
# 		make DESTDIR=${pkgdir} install
# 		popd
# 	done



 	for it in ${packs[@]} ; do

		# Remove kdepim-runtime translations
                pushd "kde-l10n-$it-$pkgver/messages/"
	          	sed -i -e "s~add_subdirectory(kdepim-runtime)~~g" CMakeLists.txt
	                rm -rfv kdepim-runtime
		popd



                pushd "kde-l10n-$it-$pkgver"

patch -Np4 -i ../../global-settings.docbook.fix.patch
 		cmake -DCMAKE_INSTALL_PREFIX=${_installprefix} -DCMAKE_BUILD_TYPE=Release
 		make || return 1
 		#make DESTDIR=${pkgdir} install
 		popd
 	done
}



#
# split-install functions
#
package_kde-l10n-ar()
{
	pkgdesc="KDE language support: Arabic"
	groups=("kde-uninstall" "kde-l10n")

	pushd ${srcdir}/kde-l10n-ar-${pkgver}
		make DESTDIR=${pkgdir} install || return 1
	popd
}

