	rm -v ${pkgdir}/usr/share/locale/sr@ijekavianlatin/LC_MESSAGES/ktorrent.mo
	rm -v ${pkgdir}/usr/share/locale/sr@latin/LC_MESSAGES/ktorrent.mo
}

