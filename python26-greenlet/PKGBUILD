# include global config
source ../_buildscripts/${current_repo}-${_arch}-cfg.conf

_pkgname=python-greenlet
pkgname=python26-greenlet
_pkgver=0.4
pkgver=0.4.0
pkgrel=1
pkgdesc="The greenlet package is a spin-off of Stackless, a version of CPython that supports micro-threads called \"tasklets\"."
arch=('i386' 'x86_64')
license=('LGPL')
depends=('python26')
makedepends=('python26-setuptools')
url="http://bitbucket.org/ambroff/greenlet"
source=("http://download.ag-projects.com/SipClient/${_pkgname}-${_pkgver}.tar.gz")
md5sums=('22f1c8d32f7decc6ead2a3a60a272fe5')

package() {
  cd "${srcdir}/${_pkgname}-${_pkgver}"
  python2.6 setup.py install --prefix=/usr --root="${pkgdir}" || return 1
}

