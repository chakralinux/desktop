name=eventlet
pkgname=python2-eventlet
pkgver=0.14.0
pkgrel=1
pkgdesc="Highly concurrent networking library"
arch=(x86_64)
url="http://eventlet.net"
license=(MIT)
depends=('python2-setuptools' 'python2-greenlet')
makedepends=('python2')
replaces=('python-eventlet')
source=("http://pypi.python.org/packages/source/e/${name}/${name}-${pkgver}.tar.gz")
md5sums=('207119abce774018432225bd719403fb')

package() {
  cd "${srcdir}/${_pkgname}-${pkgver}"
  python2 setup.py install --prefix=/usr --root="${pkgdir}"
}
