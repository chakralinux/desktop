_pkgname=python-eventlet-0.8
pkgname=python26-eventlet
pkgver=0.8.11
pkgrel=1
pkgdesc="Eventlet is a networking library written in Python."
arch=('any')
license=('LGPL')
depends=('python26' 'python26-greenlet')
url="http://wiki.secondlife.com/wiki/Eventlet"
source=(http://download.ag-projects.com/SipClient/${_pkgname}-${pkgver}.tar.gz)
md5sums=('bb0076a4792e697bf1096fc8312dab44')

package() {
  cd "${srcdir}/${_pkgname}-${pkgver}"
  python2.6 setup.py install --prefix=/usr --root="${pkgdir}" || return 1
}
